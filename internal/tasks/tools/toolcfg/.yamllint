---
extends: "default"
# Rules found at: https://yamllint.readthedocs.io/en/stable/rules.html
rules:
  line-length:
    max: 100
    level: "error"
  empty-lines:
    max: 1
    max-start: 0
    max-end: 0
    level: "error"
  quoted-strings:
    required: true
    quote-type: "double"
    level: "error"
